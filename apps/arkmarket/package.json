    "@tanstack/react-table": "^8.15.3",
    "@tanstack/react-virtual": "^3.5.0",
    "@vercel/speed-insights": "^1.0.10",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.2.6",
    "geist": "^1.2.2",
